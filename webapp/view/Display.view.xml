<mvc:View controllerName="com.study.Z_F3_InitialApp.controller.Display" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Tree id="Tree" items="{path: 'myTree>/'}">
							<StandardTreeItem title="{myTree>text}"/>
						</Tree>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>